{
  "name": "realtime-server",
  "description": "providing real-time sychronization for ruby on rails",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production supervisor server.js",
    "dev": "NODE_ENV=development supervisor --debug server.js",
    "build": "NODE_ENV=production webpack -p --config ./client/webpack.production.config.js",
    "build:dev": "webpack-dev-server --config ./client/webpack.dev.config.js --inline --devtool eval --progress --colors --hot --content-base dist",
    "debug": "node-inspector server.js",
    "clean": "rimraf client/dist",
    "deploy": "./copy-config.sh aws2;pm2 deploy ecosystem.json5 production",
    "restart": "pm2 restart server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "config": "^1.16.0",
    "express": "^4.13.3",
    "json5": "^0.4.0",
    "lodash": "*",
    "mkdirp": "^0.5.1",
    "pg": "*",
    "redis": "0.7.3",
    "slack-client": "^1.4.1",
    "socket.io": "*"
  },
  "devDependencies": {
    "airbrake-js": "^0.5.2",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "bootstrap": "^3.3.5",
    "classnames": "^2.1.3",
    "cookies-js": "^1.2.2",
    "css-loader": "^0.18.0",
    "es6-promise": "^3.0.2",
    "history": "^1.11.1",
    "imports-loader": "^0.6.4",
    "jquery": "^2.1.4",
    "moment": "^2.10.6",
    "node-sass": "^3.4.2",
    "node-uuid": "^1.4.3",
    "normalize.css": "^3.0.3",
    "object-assign": "^4.0.1",
    "parse": "^1.6.1",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-cursor": "^1.2.1",
    "react-dnd": "^1.1.5",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^3.0.1",
    "react-router": "^1.0.0-rc1",
    "react-toolbox": "^0.12.13",
    "react-transmit": "^2.6.4",
    "redux": "^3.0.2",
    "redux-devtools": "^2.1.5",
    "redux-logger": "^2.0.1",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.4.3",
    "rx-lite": "^3.1.2",
    "sass-loader": "^3.1.2",
    "socket.io-client": "^1.3.7",
    "style-loader": "^0.12.3",
    "superagent": "^1.4.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.0",
    "whatwg-fetch": "^0.9.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/mikeatlas/realtime-server.git"
  }
}
